# OpenAI API
OPENAI_API_KEY=sk-proj-...

# JWT
JWT_SECRET=your-super-secret-jwt-key-change-in-production

# Google OAuth
GOOGLE_CLIENT_ID=your-google-client-id.apps.googleusercontent.com
GOOGLE_CLIENT_SECRET=your-google-client-secret
GOOGLE_CALLBACK_URL=http://localhost:3000/auth/google/callback

# Apple OAuth (requires Apple Developer Account)
APPLE_CLIENT_ID=com.urmate.ai.zuza
APPLE_TEAM_ID=your-apple-team-id
APPLE_KEY_ID=your-apple-key-id
APPLE_PRIVATE_KEY_PATH=./certs/AuthKey_XXXXX.p8

# Server
PORT=3000

# Gmail Integration
GMAIL_REDIRECT_URI=http://localhost:3000/api/v1/integrations/gmail/callback

# Encryption for OAuth tokens (generate with: openssl rand -hex 32)
ENCRYPTION_KEY=your-encryption-key-here

# Frontend URL for OAuth redirects
FRONTEND_URL=http://localhost:8081
