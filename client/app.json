{
  "expo": {
    "name": "Asystentka AI Zuza",
    "slug": "asystentka-ai-zuza",
    "version": "1.1.0",
    "orientation": "portrait",
    "icon": "./assets/urmate.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "scheme": "urmate-ai-zuza",
    "splash": {
      "image": "./assets/urmate.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.urmate.ai.zuza",
      "infoPlist": {
        "CFBundleURLTypes": [
          {
            "CFBundleURLSchemes": [
              "urmate-ai-zuza"
            ],
            "CFBundleURLName": "com.urmate.ai.zuza"
          }
        ],
        "NSLocationWhenInUseUsageDescription": "Potrzebujemy Twojej lokalizacji, aby lepiej odpowiadać na pytania o pogodę i lokalne wydarzenia.",
        "NSContactsUsageDescription": "Potrzebujemy dostępu do Twoich kontaktów, aby Zuza mogła odpowiadać na pytania o kontakty i pomagać w wysyłaniu wiadomości.",
        "LSApplicationQueriesSchemes": [
          "urmate-ai-zuza"
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/urmate.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.urmate.ai.zuza",
      "permissions": [
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION",
        "READ_CONTACTS"
      ],
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "urmate-ai-zuza"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ]
    },
    "web": {
      "favicon": "./assets/urmate.png",
      "bundler": "metro"
    },
    "extra": {
      "eas": {
        "projectId": "d617390d-960c-4fe8-8ca3-c98d05a70e8a"
      }
    }
  }
}
